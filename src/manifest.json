{
  "name": "histree",
  "version": "1.1.2",
  "manifest_version": 2,
  "description": "Visualize your tab history as a tree",
  "homepage_url": "http://www.elliotplant.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "bg/HistreeStorage.js",
      "bg/tree_utils.js",
      "bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "histree",
    "default_popup": "browser_action/browser_action.html"
  },
  "permissions": [
    "history",
    "tabs"
  ]
}
